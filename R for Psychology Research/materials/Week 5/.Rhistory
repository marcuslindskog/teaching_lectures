library(ggplot2)
p <- ggplot(data = diamonds, mapping = aes(x = carat, y = price)) + geom_point(aes(color = color))
p + theme_bw()+theme(panel.grid = element_blank())
p + theme_bw()+theme(panel.grid = element_blank(),)
p + theme_bw()+theme(panel.grid = element_blank(), axis.line = element_blank())
p + theme_bw()+theme(panel.grid = element_blank(), axis.ticks  = element_blank())
p + theme_bw()+theme(panel.grid = element_blank(), axis.ticks  = element_blank(), panel.border = element_blank())
p <- ggplot(data = diamonds, mapping = aes(x = price)) + geom_density()
p
